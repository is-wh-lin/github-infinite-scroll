{
  "name": "github-infinite-scroll",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "build:analyze": "ANALYZE=true nuxt build",
    "build:production": "NODE_ENV=production nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "generate:analyze": "ANALYZE=true nuxt generate",
    "generate:production": "NODE_ENV=production nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "nuxt typecheck",
    "test": "vitest",
    "test:run": "vitest run",
    "deployment:status": "node scripts/check-deployment-status.js",
    "deployment:check": "npm run deployment:status",
    "bundle:analyze": "npx nuxi analyze",
    "bundle:size": "npm run build && du -sh .output/public",
    "build:check": "node scripts/check-build-optimization.js",
    "build:optimize": "npm run generate:production && npm run build:check",
    "test:deployment": "node scripts/test-deployment-workflow.js",
    "test:functionality": "node scripts/test-application-functionality.js",
    "test:triggers": "node scripts/test-deployment-triggers.js",
    "test:deployment:complete": "node scripts/test-complete-deployment.js"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.4",
    "eslint": "^9.35.0",
    "nuxt": "^4.1.2",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^18.0.1",
    "vitest": "^3.2.4"
  }
}